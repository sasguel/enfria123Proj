<?xml version='1.0' encoding='utf-8'?>
<widget id="co.edu.unisangil.nandoic" version="0.6.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <icon density="ldpi" src="www/img/ICONO.png" />
    <icon density="mdpi" src="www/img/ICONO.png" />
    <icon density="hdpi" src="www/img/ICONO.png" />
    <icon density="xhdpi" src="www/img/ICONO.png" />
    <splash density="land-hdpi" src="www/img/splash.png" />
    <splash density="land-ldpi" src="www/img/splash.png" />
    <splash density="land-mdpi" src="www/img/splash.png" />
    <splash density="land-xhdpi" src="www/img/splash.png" />
    <splash density="port-hdpi" src="www/img/splash.png" />
    <splash density="port-ldpi" src="www/img/splash.png" />
    <splash density="port-mdpi" src="www/img/splash.png" />
    <splash density="port-xhdpi" src="www/img/splash.png" />
    <allow-intent href="market:*" />
    <name>nandoic</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="8000" />
</widget>
